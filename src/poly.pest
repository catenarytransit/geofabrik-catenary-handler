name = { (!"\n" ~ ANY)+ }

separator = _{ (" " | "\t")+ }

decimal = _{ sign? ~ number ~ ("." ~ number ~ ("E" ~ sign ~ number)?)? }
sign = _{ "+" | "-" }
number = _{ ASCII_DIGIT+ }

file = { SOI ~ (shape ~ NEWLINE)* ~ "END" ~ NEWLINE* ~ EOI }

shape = { (name ~ NEWLINE)? ~ name ~ NEWLINE ~ (point ~ NEWLINE)* ~ "END" }
point = { separator? ~ x ~ separator ~ y}
x = { decimal }
y = { decimal }